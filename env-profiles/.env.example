# Copy this to local.env, prod.env, supabase.env and fill in values

# local.env - Local PostgreSQL
DATABASE_URL=postgresql://[user]@localhost:5432/iptvchannels

# prod.env - Supabase connection string
DATABASE_URL=postgresql://postgres.[ref]:[password]@aws-1-eu-north-1.pooler.supabase.com:6543/postgres

# supabase.env - CLI access token (generate at https://supabase.com/dashboard/account/tokens)
SUPABASE_ACCESS_TOKEN=sbp_xxxx...

# local.env / prod.env - Kodi JSON-RPC API (optional - for syncing contentId values)
KODI_HOST=192.168.86.221
KODI_PORT=8081
